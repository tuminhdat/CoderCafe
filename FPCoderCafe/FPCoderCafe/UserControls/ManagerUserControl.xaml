<UserControl x:Class="FPCoderCafe.UserControls.ManagerUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FPCoderCafe.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="Aqua">
    <TabControl Name="MainTabControl">
        <TabItem Header="Manage Food" Width="200">
            <local:FoodUserControl x:Name="FoodControl"/>
        </TabItem>
        <TabItem Header="Manage Category" Width="200">
            <local:CategoryUserControl/>
        </TabItem>
        <TabItem Header="Control Panel" Width="200">
            <local:ControlPanelUserControl/>
        </TabItem>
    </TabControl>
</UserControl>
